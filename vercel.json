{
    "version": 2,
    "builds": [
      { 
        "src": "server/app.js", 
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server/app.js" }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}