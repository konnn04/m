{
  "version": 2,
  "builds": [
    {
      "src": "server/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "server/app.js"
    },
    {
      "src": "(.*)",
      "dest": "index.html"
    }
  ],
  "env": {
    "YTDLP_PATH": "server/lib/yt-dlp"
  },
  "buildCommand": "npm run postinstall",
  "functions": {
    "server/app.js": {
      "includeFiles": "server/lib/**"
    }
  }
}